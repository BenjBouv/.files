[diff]
git = 1
unified = 8
showfunc = 1

[ui]
username = Benjamin Bouvier <benj@benj.me>
editor = vim

[paths]
try = ssh://hg.mozilla.org/try

[alias]
try = push -f try

[defaults]
qnew = -U
