[extensions]
mq=
rebase=
progress=
color=
pager=
graphlog=
histedit =
qbackout=~/.files/hgplugins/qbackout
crecord=~/.files/hgplugins/crecord
trychooser = ~/.files/hgplugins/trychooser/trychooser
bzexport = ~/.mozbuild/version-control-tools/hgext/bzexport
mqext = ~/.mozbuild/mercurial/extensions/mqext
qimportbz = ~/.mozbuild/version-control-tools/hgext/qimportbz
